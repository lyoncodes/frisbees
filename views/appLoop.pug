each item in Data.frisbees
  if item.img_slug
    .m-container.m-center.mb-1.profile-pic
      img(src=`./assets/imgs/${item.img_slug}`)
  .col-12.row.m-container.m-center.p-0
    .col-sm-12.col-md-10.col-4.p-0
      section-heading
        #title(slot='title') #{item.title}
        .text-block(slot='textBlock') #{item.content_text}
      if item.btn
        .row.m-container.m-center
          button.mt-1
            a(href=`${item.btn.btn_href}`) #{item.btn.btn_text}
      if item.i_row
        include icon-row
  if item.carousel_slug
    .m-container.m-center
      .col-sm-12.col-md-8.col-4
        m-carousel.mt-1
  #ben
  if item.u_lists
    .col-12
      include unordered-list